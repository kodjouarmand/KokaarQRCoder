
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<br />


<div class="row">
    <div class="col-6">
        <h2 class="text-primary">Agent List</h2>
    </div>
    <div class="col-6 text-right">
        <a class="btn btn-primary" asp-action="Upsert"><i class="fas fa-plus"></i> &nbsp; Add new agent</a>
        <a class="btn btn-primary" asp-action="Import"><i class="fas fa-upload"></i> &nbsp; Import from file</a>
    </div>
</div>

<br />
<div class="p-4 border rounded">
    <table id="tblData" class="table table-striped table-bordered table-sm" style="width:100%">
        <thead class="thead-dark">
            <tr class="table-info">
                <th>Company</th>
                <th>Number</th>
                <th>Name</th>
                <th>Phone Number</th>
                <th>Email</th>
                <th></th>
            </tr>
        </thead>
    </table>
</div>

@section Scripts{

    <script src="~/js/agent.js"></script>
}
